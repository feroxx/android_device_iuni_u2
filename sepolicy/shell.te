# address denials with some CTS cases
allow shell persist_file:dir r_dir_perms;
allow shell sysfs_battery_supply:dir r_dir_perms;
allow shell sysfs_usb_supply:dir r_dir_perms;
allow shell sysfs:file { open read getattr };
allow shell sysfs_vibrator:dir { open read search };
allow shell sysfs_vibrator:file rw_file_perms;
allow shell sysfs_usb_supply:file r_file_perms;
allow shell sysfs_uio_file:file r_file_perms;
allow shell sysfs_mmc_host:file r_file_perms;
